    "docs": "typedoc --out docs src/index.ts",
    "clean": "rimraf dist"
  },
  "keywords": ["agentos", "guardrail", "safety", "pii", "privacy", "redaction"],
  "author": "Frame.dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/framersai/agentos-guardrails.git",
