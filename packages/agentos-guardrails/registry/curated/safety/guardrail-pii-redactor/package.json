  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@agentos/core": "^0.1.0"
  },
  "devDependencies": {
    "@agentos/core": "workspace:*",
    "@types/node": "^20.12.12",
    "eslint": "^9.11.0",
    "rimraf": "^5.0.7",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}

