{
  "name": "@framers/agentos-communications-telegram",
  "version": "1.0.0",
  "description": "Telegram Bot API extension for AgentOS - manage channels, send messages, and interact as a bot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src test --ext .ts",
    "lint:fix": "eslint src test --ext .ts --fix",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build",
    "docs": "typedoc --out docs src/index.ts"
  },
  "keywords": [
    "agentos",
    "extension",
    "telegram",
    "bot",
    "messaging",
    "communications",
    "chat"
  ],
  "author": "Frame.dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/framersai/agentos-extensions",
    "directory": "registry/curated/communications/telegram-bot"
  },
  "files": [
    "dist",
    "manifest.json",
    "README.md"
  ],
  "peerDependencies": {
    "@agentos/core": "^2.0.0"
  },
  "dependencies": {
    "node-telegram-bot-api": "^0.64.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@agentos/core": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/node-telegram-bot-api": "^0.64.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "typedoc": "^0.25.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
