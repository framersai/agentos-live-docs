{
  "name": "voice-chat-assistant-backend",
  "version": "1.0.0",
  "description": "Backend for Voice Chat Assistant with AgentOS integration",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch --clear-screen=false server.ts",
    "dev:debug": "tsx watch --inspect server.ts",
    "build": "npm run db:generate && tsc",
    "build:docker": "docker build -t voice-chat-assistant-backend .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:push": "prisma db push",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rm -rf dist",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@lemonsqueezy/lemonsqueezy.js": "^3.2.0",
    "@pinecone-database/pinecone": "^3.0.0",
    "@prisma/client": "^5.20.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "google-auth-library": "^9.15.1",
    "helmet": "^7.1.0",
    "i18next": "^23.15.2",
    "i18next-fs-backend": "^2.3.2",
    "i18next-http-middleware": "^3.6.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^8.0.1",
    "node-cron": "^3.0.3",
    "openai": "^4.67.3",
    "prisma": "^5.20.0",
    "redis": "^4.7.0",
    "stripe": "^16.12.0",
    "uuid": "^10.0.0",
    "weaviate-ts-client": "^2.2.0",
    "winston": "^3.15.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.22",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/natural": "^5.1.5",
    "@types/node": "^22.7.5",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitest/coverage-v8": "^2.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.6.3",
    "vitest": "^2.1.2"
  }
}
