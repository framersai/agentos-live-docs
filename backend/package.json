{
  "name": "voice-chat-assistant-backend",
  "version": "0.1.0",
  "description": "Backend for voice coding assistant",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node dist/server.js",
    "dev": "tsx watch server.ts",
    "build": "tsc -p tsconfig.build.json || echo 'TypeScript compilation had warnings but continuing...' && tsc-alias -p tsconfig.build.json",
    "build:strict": "tsc && tsc-alias",
    "build:agentos": "tsc && tsc-alias",
    "build:shared": "tsc -p ../shared/tsconfig.build.json",
    "postbuild": "npm run build:shared",
    "test": "node --test --import tsx --experimental-specifier-resolution=node src/__tests__/appDatabase.test.ts"
  },
  "dependencies": {
    "@framers/agentos": "workspace:*",
    "@framers/sql-storage-adapter": "workspace:*",
    "@supabase/supabase-js": "^2.45.1",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.10.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "i18next": "^25.2.1",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.7.4",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "socket.io": "^4.8.1",
    "tslib": "^2.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.10",
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/framersai/agentos-backend"
  }
}
