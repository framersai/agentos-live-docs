{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./lib/crypto.ts",
    "./lib/email.ts",
    "./lib/rateLimit.ts",
    "./lib/supabaseServer.ts",
    "./lib/supabaseBrowser.ts",
    "../../packages/gitpaywidget-sdk/src/index.ts",
    "../../packages/gitpaywidget-widget/src/index.ts",
    "../../packages/gitpaywidget-widget/src/types.ts",
    "../../packages/payments-core/src/index.ts",
    "../../packages/payments-core/src/types.ts"
  ],
  "out": "./public/docs/api",
  "name": "GitPayWidget API Documentation",
  "readme": "./README.md",
  "theme": "default",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "includeVersion": true,
  "navigationLinks": {
    "GitPayWidget": "https://gitpaywidget.com",
    "GitHub": "https://github.com/manicinc/gitpaywidget",
    "Docs": "https://gitpaywidget.com/docs"
  },
  "sidebarLinks": {
    "Quick Start": "https://gitpaywidget.com/docs/quickstart",
    "API Reference": "https://gitpaywidget.com/docs/api",
    "Integration Guide": "https://gitpaywidget.com/docs/integration"
  },
  "plugin": [],
  "sort": ["source-order"],
  "categorizeByGroup": true,
  "categoryOrder": ["Core", "SDK", "Widget", "Providers", "Utilities", "*"],
  "searchInComments": true,
  "excludeExternals": true,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  }
}


