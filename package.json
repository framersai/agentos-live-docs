{
    "name":  "voice-coding-assistant",
    "version":  "1.0.0",
    "description":  "A real-time voice-driven coding assistant web app",
    "main":  "index.js",
    "scripts":  {
                    "install-all":  "npm install \u0026\u0026 cd frontend \u0026\u0026 npm install \u0026\u0026 cd ../backend \u0026\u0026 npm install \u0026\u0026 cd ../packages/agentos \u0026\u0026 npm install",
                    "dev":  "concurrently \"cd backend \u0026\u0026 npm run dev\" \"cd frontend \u0026\u0026 npm run dev\"",
                    "start:dev":  "npm run dev",
                    "start":  "concurrently \"cd backend \u0026\u0026 npm start\" \"cd frontend \u0026\u0026 npm run preview -- --host 0.0.0.0\"",
                    "start:prod":  "npm run build \u0026\u0026 npm run start",
                    "build":  "npm run build:frontend \u0026\u0026 npm run build:backend",
                    "build:frontend":  "npm --prefix frontend run build",
                    "build:backend":  "npm --prefix packages/agentos run build \u0026\u0026 npm --prefix backend run build",
                    "build:landing":  "pnpm --filter @wearetheframers/agentos-landing build",
                    "build:agentos-client":  "pnpm --filter @wearetheframers/agentos-client build",
                    "dev:landing":  "pnpm --filter @wearetheframers/agentos-landing dev",
                    "dev:agentos-client":  "pnpm --filter @wearetheframers/agentos-client dev",
                    "test":  "echo \"Error: no test specified\" \u0026\u0026 exit 1"
                },
    "keywords":  [
                     "voice",
                     "coding",
                     "assistant",
                     "speech-to-text",
                     "AI"
                 ],
    "author":  "",
    "license":  "MIT",
    "devDependencies":  {
                            "@types/natural":  "^5.1.5",
                            "concurrently":  "^8.2.2",
                            "postcss-nesting":  "^13.0.1"
                        },
    "dependencies":  {
                         "highlight.js":  "^11.11.1",
                         "jszip":  "^3.10.1",
                         "stemmer":  "^2.0.1",
                         "pino":  "^9.3.0"
                     },
    "packageManager":  "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
